//

GameInfo {
	// Map handler should be also defined here because of the "Force use map 
	//scripts" CVar existence.

	AddEventHandlers = "ZCheckerHandler", "ZCheckerEverythingMapHandler"
}

Map EVERYTHING_SIMPLE "$ZCHECKER_MAPTITLE_EVERYTHING_SIMPLE" {
	Sky1 = "RSKY3"
	Music = ""
	Cluster = 0
	NoAutosaveHint
}

Map EVERYTHING_SC77 "$ZCHECKER_MAPTITLE_EVERYTHING_ALT1" {
	Sky1 = "RSKY3"
	Music = ""
	Cluster = 0
	NoAutosaveHint
}
